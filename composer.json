{
    "name": "pbmedia/laravel-ffmpeg",
    "description": "FFMpeg for Laravel",
    "keywords": [
        "pbmedia",
        "laravel",
        "laravel-ffmpeg",
        "ffmpeg",
        "protonemedia",
        "protone media",
        "pascal baljet media",
        "pascalbaljetmedia"
    ],
    "homepage": "https://github.com/pascalbaljetmedia/laravel-ffmpeg",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@protone.media",
            "homepage": "https://protone.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "evenement/evenement": "^3.0",
        "illuminate/bus": "^9.0",
        "illuminate/config": "^9.0",
        "illuminate/filesystem": "^9.0",
        "illuminate/log": "^9.0",
        "illuminate/support": "^9.0",
        "protonemedia/php-ffmpeg": "^1.2"
    },
    "require-dev": {
        "league/flysystem-memory": "^3.0",
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5.10",
        "spatie/image": "^2.0",
        "spatie/phpunit-snapshot-assertions": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "ProtoneMedia\\LaravelFFMpeg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ProtoneMedia\\LaravelFFMpeg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ProtoneMedia\\LaravelFFMpeg\\Support\\ServiceProvider"
            ],
            "aliases": {
                "FFMpeg": "ProtoneMedia\\LaravelFFMpeg\\Support\\FFMpeg"
            }
        }
    }
}